{
  "name": "learn-purescript",
  "version": "1.0.0",
  "description": "Playground for learning Purescript",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "test": "spago test",
    "web-dialog-build": "spago bundle-app --main App.Web.Dialog.Main --to dist/App/Web/Dialog/main.js",
    "web-dialog-watch": "spago bundle-app --watch --main App.Web.Dialog.Main --to dist/App/Web/Dialog/main.js",
    "web-dialog-serve": "http-server dist/App/Web/Dialog -o",
    "web-dialog-start": "npm run web-dialog-build && npm run web-dialog-serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Unisay/learn-purescript.git"
  },
  "keywords": [
    "purescript",
    "fp",
    "spago"
  ],
  "author": "Yuriy Lazarev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Unisay/learn-purescript/issues"
  },
  "homepage": "https://github.com/Unisay/learn-purescript#readme",
  "devDependencies": {
    "http-server": "^13.0.0",
    "purescript": "^0.14.1",
    "purs-tidy": "^0.6.2",
    "purty": "^7.0.0",
    "spago": "^0.19.2"
  }
}
